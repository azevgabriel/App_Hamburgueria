{
  "expo": {
    "name": "Bogiatto Cupons",
    "slug": "BogiattoCupons",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#fbe192",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fbe192"
