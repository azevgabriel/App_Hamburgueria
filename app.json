    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fbe192"
      },
      "package" : "com.aldeia.bogiattocupons"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
