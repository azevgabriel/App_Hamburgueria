{
  "expo": {
    "name": "a",
    "slug": "a",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#fbe192",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fbe192"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fbe192"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
